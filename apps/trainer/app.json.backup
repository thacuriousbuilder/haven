        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#206E6B",
      "androidMode": "default",
      "androidCollapsedTitle": "HAVEN Coach Updates"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#206E6B"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c4c2ea3b-3057-4367-8c23-1923327c23ca"
