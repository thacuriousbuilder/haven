
{
    "cli": {
      "version": ">= 5.0.0"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "env": {
          "APP_VARIANT": "development"
        },
        "ios": {
          "simulator": false
        }
      },
      "preview": {
        "distribution": "internal",
        "env": {
          "APP_VARIANT": "preview"
        },
        "ios": {
          "simulator": false
        }
      },
      "production": {
        "autoIncrement": true,
        "env": {
          "APP_VARIANT": "production"
        },
        "ios": {
          "buildConfiguration": "Release"
        },
        "android": {
          "buildType": "app-bundle"
        }
      }
    },
    "submit": {
        "production": {
          "ios": {
